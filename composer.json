    "type": "library",
    "license": "BSD-3-Clause",
    "keywords": [
        "zf",
        "zf3",
        "zendframework",
        "partial"
    ],
    "authors": [
        {
            "name": "Nic Davies",
            "email": "nic.davies.dev@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0",
